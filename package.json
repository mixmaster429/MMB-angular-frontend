{
  "name": "workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=en",
    "start:proxy": "ng serve --configuration=en --proxy-config=proxy.conf.json",
    "start:admin:proxy": "ng serve --project mmb-admin-app --proxy-config=proxy.conf.json",
    "start:org:proxy": "npm run json-server:org | ng serve --project mmb-org-app --proxy-config=proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "npx compodoc -p tsconfig.json -n \"Movemeback documentation\" -s",
    "styleguide": "ng serve --project mmb-styleguide-app --port 4300",
    "extract-i18n": "ng xi18n --output-path=src && xliffmerge",
    "json-server": "json-server --watch mock-data/db.json --routes mock-data/routes.json --port 3000",
    "json-server:org": "json-server --watch mock-data/org-db.json --routes mock-data/org-routes.json --port 3000"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
